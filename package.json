{
  "name": "css-js-gen",
  "version": "1.0.0",
  "description": "Convert CSS object notation to CSS strings",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "dev": "tsdown --config tsdown.config.ts --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "scratchpad:test-styles": "node test-styles.mjs"
  },
  "keywords": [
    "css",
    "css-in-js",
    "styles",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.17.0",
  "devDependencies": {
    "@vitest/ui": "^4.0.18",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}